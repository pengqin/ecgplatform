<?xml version="1.0" encoding="utf-8"?>
<urlrewrite>
  <rule>
    <from>api/user/([0-9]+)/examination/([0-9]+)/ecg([1-7]).png</from>
    <to type="forward">upload/heart_img/user/$1/examination/$2/ecg$3.png</to>
  </rule>
  <rule>
    <from>api/user/([0-9]+)/examination/([0-9]+)/raw</from>
    <to type="forward">upload/heart_img/user/$1/examination/$2/raw</to>
  </rule>
</urlrewrite>